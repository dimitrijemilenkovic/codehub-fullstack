{
  "name": "codehub-fullstack",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd codehub-react && npm run dev",
    "dev:server": "cd server && npm run dev",
    "install:all": "npm install && cd codehub-react && npm install && cd ../server && npm install",
    "build": "cd codehub-react && npm run build",
    "start": "cd server && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
